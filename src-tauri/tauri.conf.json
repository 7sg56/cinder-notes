{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "cinder-notes",
  "version": "0.1.2",
  "identifier": "com.cindernotes.main",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "cinder",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/svg/32 x 32.svg",
      "icons/svg/64 x 64.svg",
      "icons/svg/128 x 128.svg",
      "icons/svg/256 x 256.svg"
    ]
  }
}
